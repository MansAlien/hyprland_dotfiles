{
   "action": {
      "default_popup": "popup.html",
      "default_title": "Friendly YouTube Adblock"
   },
   "background": {
      "service_worker": "background.js"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "content-scripts/youtube-ad-blocker.js" ],
      "matches": [ "*://*/*" ],
      "run_at": "document_start"
   } ],
   "description": "Block ads on YouTube. Auto skip video ads. Auto hide banner ads. Our Adblocker Skipper also blocks ads in embedded videos.",
   "icons": {
      "128": "icon/128.png",
      "16": "icon/16.png",
      "32": "icon/32.png",
      "48": "icon/48.png",
      "96": "icon/96.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "YouTube Adblock by Friendly",
   "permissions": [ "storage", "tabs" ],
   "short_name": "Friendly",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0.1"
}
